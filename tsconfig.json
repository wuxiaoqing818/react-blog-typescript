{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "experimentalDecorators": true
  },
  "include": [
    "src"
  ],
  "baseUrl": ".",
  "paths": {
    "@*": [
      "src*"
    ],
    "@typings*": [
      "src/typings*"
    ],
    "@components*": [
      "src/components*"
    ],
    "@utils*": [
      "src/utils*"
    ],
    "@router*": [
      "src/router*"
    ],
    "@store*": [
      "src/store*"
    ],
    "@services*": [
      "src/services*"
    ]
  },
  "extends": "./tsconfig.extend.json"
}
